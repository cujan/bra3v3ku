                      vseobecneUdaje.nazovOrganizacie, vseobecneUdaje.ulica, vseobecneUdaje.ulicaCislo, vseobecneUdaje.mesto, vseobecneUdaje.psc, 
                      vseobecneUdaje.ico, vseobecneUdaje.dic, vseobecneUdaje.telefon, vseobecneUdaje.mobil, vseobecneUdaje.email, vseobecneUdaje.www
FROM         vseobecneUdaje, ((((((osoba LEFT OUTER JOIN
                      student ON osoba.id = student.id) LEFT OUTER JOIN
                      clenKurzu ON clenKurzu.idStudent = student.id) LEFT OUTER JOIN
                      kurz ON kurz.id = clenKurzu.idKurz) LEFT OUTER JOIN
                      platba ON platba.idStudent = student.id) LEFT OUTER JOIN
                      studentSkupinaUchadzac ON studentSkupinaUchadzac.idStudent = student.id) LEFT OUTER JOIN
                      studentSkupinaVlastnik ON studentSkupinaVlastnik.idStudent = student.id)</CommandText>
