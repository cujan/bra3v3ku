            <SelectCommand>
              <DbCommand CommandType="Text" ModifiedByUser="true">
                <CommandText>SELECT     osoba.meno, osoba.priezvisko, osoba.datumNarodenia, osoba.miestoNarodenia, osoba.ulica AS Expr1, osoba.ulicaCislo AS Expr2, 
